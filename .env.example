# TCG eBay Batch Uploader Environment Variables
# Copy this file to .env and fill in your actual values

# Ximilar API Configuration
XIMILAR_API_KEY=your_ximilar_api_key_here
XIMILAR_ENDPOINT=https://api.ximilar.com/collectibles/v2/tcg_id

# Pokemon TCG API Configuration
POKEMON_TCG_API_KEY=your_pokemon_tcg_api_key_here

# OpenAI API Configuration (optional - for title optimization)
OPENAI_API_KEY=your_openai_api_key_here

# eBay API Configuration
EBAY_APP_ID=your_ebay_app_id_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_USER_TOKEN=your_ebay_user_token_here

# Business Policies (eBay) - Update with your policy names
PAYMENT_POLICY_NAME=Immediate Payment (BIN)
SHIPPING_POLICY_UNDER_20=Standard Envelope 1oz (Free)
SHIPPING_POLICY_OVER_20=Free Shipping US GA
RETURN_POLICY_NAME=Returns Accepted